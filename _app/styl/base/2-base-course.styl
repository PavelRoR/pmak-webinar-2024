*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: I
	color #fff
	background-color #000
	line-height: 1.3
.wrapper
	overflow hidden
.content
	position relative
	z-index 60
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display block
	text-align center
	margin 0 auto
	text-decoration none
	color: #fff
	width: 100%
	max-width 391px
	font-size: 26px
	font-weight: bold
	padding: 27px
	background-color: #C6F9DC
	background-image: linear-gradient(22deg, #C6F9DC 0%, #4E97B6 100%)
	border-radius: 42px
	line-height: 1
	@media screen and (max-width 386px)
		font-size: 18px
		padding 20px
	&:focus
		text-decoration none
		color #fff
	&:hover
		text-decoration none
		color #fff
.general-title
	font-family: S
	font-weight: bold
	font-size: 60px
	margin-bottom: 60px
	color #000
	line-height: 1.3
	@media screen and (max-width 991px)
		font-size: 50px
	@media screen and (max-width 560px)
		font-size: 32px
		margin-bottom: 40px
	@media screen and (max-width 386px)
		font-size: 26px
	&-white
		color #fff
	span
		color #40BDAE
.mob-none
	@media screen and (max-width 767px)
		display none !important
.mob-none-560
	@media screen and (max-width 560px)
		display none !important
.d-none
	display none !important
	@media screen and (max-width 767px)
		display block !important
.d-none-560
	display none !important
	@media screen and (max-width 560px)
		display block !important
.general-list
	margin-left: 18px
	li
		margin-bottom: 20px
		@media screen and (max-width 386px)
			margin-bottom: 10px
		span
			font-weight: bold
		&:last-child
			margin-bottom: 0
	&-black
		color #000