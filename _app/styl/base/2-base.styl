*
	margin 0
	padding 0
	outline none
body
	font-size: 16px
	font-family: I
	color #000
	background-color #fff
	line-height: 1.1
.wrapper
	overflow hidden
.content
	position relative
	z-index 60
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display inline-block
	text-align center
	margin 0
	text-decoration none
	color: #fff
	width: auto
	font-size: 18px
	font-weight: bold
	padding: 16px 54px
	background-color: #C77E55
	border-radius: 15px
	@media screen and (max-width 991px)
		width 100%
		max-width 550px
		margin 0 auto
	&:focus
		text-decoration none
		color #fff
	&:hover
		text-decoration none
		color #fff
.general-title
	font-family: FSE
	font-size: 36px
	margin-bottom: 80px
	color #73431F
.mob-none
	@media screen and (max-width 991px)
		display none
.general-subtitle
	font-size: 18px
	margin: 60px 0 20px
	span
		display block
		color #77CEB4
		font-weight: bold
		text-transform uppercase
		&.gs-brown
			color #C77E55
.general-list
	margin-left: 30px
	@media screen and (max-width 386px)
		margin-left: 15px
	li
		position relative
		margin-bottom: 15px
		span
			font-weight: bold
		&:last-child
			margin-bottom: 0