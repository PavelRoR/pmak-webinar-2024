#for
	padding 260px 0
	margin 0 0 -60px
	background-color #fff
	border-radius: 58px 58px 0 0
	position relative
	z-index 41
.for-item
	margin-bottom: 100px
	&-title
		display flex
		flex-direction: row
		align-items: center
		justify-content: flex-start
		width 100%
		max-width 666px
		background-color: #C6F9DC
		padding 16px 40px
		border-radius: 92px
		margin-bottom: 30px
		h4
			font-size: 28px
			color #40BDAE
			margin-left: 40px